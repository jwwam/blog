<script language="javascript">
		//函数说明:搜索一个字符串在另一个字符串中第一次出现的位置
		function Search(str1,str2){
		   var r;                   // 声明变量。
		   var s = str1;
		   r = s.search(str2);            // 查找字符串。
		   return(r);                   // 返回 Boolean 结果。
		}
		//把MAC间的分号变成空格
		function Replace(str1)
		{
		   var r, re;                    // 声明变量。
		   re = /:/g;             // 创建正则表达式模式。
		   r = str1.replace(re, " ");    // 用 "A" 替换 "The"。
		   return(r);                   // 返回替换后的字符串。
		}
		</script>



